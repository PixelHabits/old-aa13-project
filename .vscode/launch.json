{
	/* 
      This configuration is for launching the Next.js application
      using the Bun package manager. But it needs to be updated to the right app
      */
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Next.js",
			"type": "node-terminal",
			"request": "launch",
			"command": "bun dev",
			"cwd": "${workspaceFolder}/apps/web/",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
